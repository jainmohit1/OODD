<p id="notice"><%= notice %></p>

<h1>House Interest Lists</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>House</th>
      <th>Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @house_interest_lists.each do |house_interest_list| %>
      <tr>
        <td><%= house_interest_list.user %></td>
        <td><%= house_interest_list.house %></td>
        <td><%= house_interest_list.company %></td>
        <td><%= link_to 'Show', house_interest_list %></td>
        <td><%= link_to 'Edit', edit_house_interest_list_path(house_interest_list) %></td>
        <td><%= link_to 'Destroy', house_interest_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New House Interest List', new_house_interest_list_path %>
